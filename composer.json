{
    "name": "yosymfony/config-loader",
    "description": "Configuration loader for YAML, TOML and JSON files",
    "keywords": ["config", "configuration", "yaml", "toml", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com",
            "homepage": "http://yosymfony.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "^2.7|^3.0"
    },
    "require-dev": {
        "yosymfony/toml": "0.3.*@dev",
        "symfony/yaml": "^2.7|^3.0",
        "phpunit/phpunit": "^4.8|^6.5|^7.0"
    },
    "suggest": {
        "yosymfony/toml": "0.3.*",
        "symfony/yaml": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": { "Yosymfony\\ConfigLoader\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Yosymfony\\ConfigLoader\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
